@using System.Web.Optimization
@using MapBul.Web.Resource


<div class="tabs-container">
    <ul class="nav nav-tabs">
        <li class="active"><a data-toggle="tab" href="#tab-1">@Resource.DicPinsCategory</a></li>
        <li class=""><a data-toggle="tab" href="#tab-2">@Resource.DicRegionsAndCities</a></li>
        <li class=""><a data-toggle="tab" href="#tab-3">@Resource.DicArticleCategoryes</a></li>

    </ul>
    <div class="tab-content">
        <div id="tab-1" class="tab-pane active">
            <div class="panel-body" id="MarkerCategoriesContainer">
                @Html.Action("_MarkerCategoriesPartial", "Dictionaries")
            </div>
        </div>
        <div id="tab-2" class="tab-pane">
            <div class="panel-body" id="CitiesContainer">
                @Html.Action("_CitiesPartial", "Dictionaries")
            </div>
        </div>
        <div id="tab-3" class="tab-pane">
            <div class="panel-body" id="ArticleCategoriesContainer">
                @Html.Action("_ArticleCategoriesPartial", "Dictionaries")
            </div>
        </div>
    </div>

</div>

@section Styles{
    @Styles.Render("~/Content/plugins/dataTables/dataTablesStyles")
    @Styles.Render("~/plugins/dataPickerStyles")
    @Styles.Render("~/Content/plugins/chosen/chosenStyles")
    @Styles.Render("~/plugins/switcheryStyles")
    @Styles.Render("~/Content/plugins/colorpicker/colorpickerStyles")
}

@section Scripts{
    @Scripts.Render("~/plugins/validate")
    @Scripts.Render("~/plugins/dataTables")
    @Scripts.Render("~/plugins/dataPicker")
    @Scripts.Render("~/plugins/chosen")
    @Scripts.Render("~/plugins/switchery")
    @Scripts.Render("~/plugins/nestable") 
    @Scripts.Render("~/plugins/colorpicker")
    @Scripts.Render(JavaScriptResource.Cities)
    @*@Scripts.Render("~/CustomScripts/Cities")*@
    @Scripts.Render(JavaScriptResource.Categories)
    @*@Scripts.Render("~/CustomScripts/Categories")*@
<script type="text/javascript" src="http://maps.googleapis.com/maps/api/js?libraries=places&sensor=false"></script>

}